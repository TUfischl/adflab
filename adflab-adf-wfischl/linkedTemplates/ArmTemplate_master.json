{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adflab-adf-wfischl"
		},
		"SSISDB Azure Endpoint_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SSISDB Azure Endpoint'"
		},
		"wfischl_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'wfischl_blob'"
		},
		"AzStorage_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzStorage_Staging'"
		},
		"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "adflabsql424zi.database.windows.net"
		},
		"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "labadmin"
		},
		"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"Blob_Source_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "MASTER.txt"
		},
		"Blob_Source_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "s3-data"
		},
		"Blob_Sink_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "FAAmaster.txt"
		},
		"Blob_Sink_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "input/FAAmaster"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SSISDB Azure Endpoint_connectionString": {
						"value": "[parameters('SSISDB Azure Endpoint_connectionString')]"
					},
					"wfischl_blob_connectionString": {
						"value": "[parameters('wfischl_blob_connectionString')]"
					},
					"AzStorage_Staging_connectionString": {
						"value": "[parameters('AzStorage_Staging_connectionString')]"
					},
					"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('ADFLab-SSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"Blob_Source_properties_typeProperties_fileName": {
						"value": "[parameters('Blob_Source_properties_typeProperties_fileName')]"
					},
					"Blob_Source_properties_typeProperties_folderPath": {
						"value": "[parameters('Blob_Source_properties_typeProperties_folderPath')]"
					},
					"Blob_Sink_properties_typeProperties_fileName": {
						"value": "[parameters('Blob_Sink_properties_typeProperties_fileName')]"
					},
					"Blob_Sink_properties_typeProperties_folderPath": {
						"value": "[parameters('Blob_Sink_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}