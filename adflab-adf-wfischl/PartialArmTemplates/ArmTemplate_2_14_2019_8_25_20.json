						"type": "String"
					},
					{
						"name": "Region",
						"type": "String"
					},
					{
						"name": "County",
						"type": "String"
					},
					{
						"name": "Country",
						"type": "String"
					},
					{
						"name": "Last Activity Date",
						"type": "String"
					},
					{
						"name": "Cert Issue Date",
						"type": "String"
					},
					{
						"name": "Certification Requested",
						"type": "String"
					},
					{
						"name": "Type Aircraft",
						"type": "String"
					},
					{
